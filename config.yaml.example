# 框架配置 - 复制此文件为 config.yaml 并填入实际值

# LLM 配置
llm:
  provider: openai
  model: deepseek-chat
  api_key: your-api-key-here
  base_url: https://api.deepseek.com/v1  # OpenAI 兼容接口地址

# 数据库配置
database:
  path: data/app.db

# 向量库配置（预留）
vector_store:
  enabled: false
  provider: chroma
  path: data/vectors
